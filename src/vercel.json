{
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "blog_app.settings"
    },
    "functions": {
      "api/*": {
        "memory": 128,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/:1"
      }
    ],
    "python": {
      "version": "3.11",
      "install": "venv/bin/python"
    }
  }
  