{
    "version": 2,
    "builds": [
      { "src": "manage.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "http://localhost:8000" }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "blog_app.settings"
    },
    "frameworks": {
      "python": {
        "buildCommand": "python -m pip install -r requirements.txt && ./env/Scripts/python.exe manage.py runserver",
        "startCommand": "./env/Scripts/python.exe manage.py runserver"
      }
    }
  }
  